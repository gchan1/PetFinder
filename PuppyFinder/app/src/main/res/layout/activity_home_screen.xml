<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="387dp"
        android:layout_height="16dp"
        android:background="@layout/round_corners"
        android:clickable="false"
        android:visibility="visible"
        app:srcCompat="@color/colorPrimary"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="102dp" />

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="android.support.design.widget.AppBarLayout$ScrollingViewBehavior"
        tools:background="@color/cast_expanded_controller_seek_bar_progress_background_tint_color">

        <LinearLayout
            android:id="@+id/container"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:orientation="vertical"
            tools:context="com.example.group1.puppyfinder.HomeScreen">

            <android.support.constraint.ConstraintLayout
                android:id="@+id/content"
                android:layout_width="match_parent"
                android:layout_height="85dp"
                android:layout_weight="1"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="0dp">


                <ImageButton
                    android:id="@+id/imageButton5"
                    android:layout_width="37dp"
                    android:layout_height="37dp"
                    android:layout_marginEnd="24dp"
                    android:layout_marginLeft="26dp"
                    android:layout_marginRight="24dp"
                    android:layout_marginStart="26dp"
                    android:layout_marginTop="32dp"
                    android:background="@layout/round_corners"
                    android:backgroundTint="@color/colorPrimaryDark"
                    android:tint="@color/colorAccent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintLeft_toRightOf="@+id/imageView3"
                    app:layout_constraintRight_toLeftOf="@+id/buttonEvents"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@android:drawable/ic_menu_mylocation"
                    tools:layout_constraintLeft_creator="1"
                    tools:layout_constraintRight_creator="1"
                    tools:layout_constraintTop_creator="1" />

                <ImageButton
                    android:id="@+id/buttonMap"
                    android:layout_width="37dp"
                    android:layout_height="37dp"
                    android:layout_marginLeft="83dp"
                    android:layout_marginStart="82dp"
                    android:background="@layout/round_corners"
                    android:backgroundTint="@color/colorPrimaryDark"
                    android:tint="@color/colorAccent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/buttonShelters"
                    app:layout_constraintVertical_bias="0.0"
                    app:srcCompat="@android:drawable/ic_menu_mylocation"
                    tools:layout_constraintBottom_creator="1"
                    tools:layout_constraintLeft_creator="1"
                    tools:layout_constraintTop_creator="1" />

                <ImageButton
                    android:id="@+id/buttonEvents"
                    android:layout_width="37dp"
                    android:layout_height="37dp"
                    android:layout_marginEnd="30dp"
                    android:layout_marginRight="28dp"
                    android:layout_marginTop="32dp"
                    android:background="@layout/round_corners"
                    android:backgroundTint="@color/colorPrimaryDark"
                    android:cropToPadding="false"
                    android:tint="@color/colorAccent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@android:drawable/ic_menu_my_calendar"
                    tools:layout_constraintRight_creator="1"
                    tools:layout_constraintTop_creator="1" />

                <ImageButton
                    android:id="@+id/buttonShelters"
                    android:layout_width="37dp"
                    android:layout_height="37dp"
                    android:layout_marginEnd="29dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="29dp"
                    android:layout_marginStart="16dp"
                    android:background="@layout/round_corners"
                    android:backgroundTint="@color/colorPrimaryDark"
                    android:tint="@color/colorAccent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toLeftOf="@+id/buttonMap"
                    app:layout_constraintTop_toTopOf="@+id/buttonEvents"
                    app:layout_constraintVertical_bias="0.0"
                    app:srcCompat="@drawable/ic_home_black_24dp"
                    tools:layout_constraintBottom_creator="1"
                    tools:layout_constraintLeft_creator="1"
                    tools:layout_constraintRight_creator="1"
                    tools:layout_constraintTop_creator="1" />

                <ImageView
                    android:id="@+id/topBar"
                    android:layout_width="0dp"
                    android:layout_height="12dp"
                    android:layout_marginTop="38dp"
                    android:background="@color/colorPrimary"
                    android:clickable="false"
                    android:visibility="visible"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="parent"
                    app:srcCompat="@color/colorPrimary"
                    tools:layout_constraintLeft_creator="1"
                    tools:layout_constraintRight_creator="1"
                    tools:layout_constraintTop_creator="1" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="74dp"
                    android:layout_height="66dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@mipmap/ic_launcher"
                    tools:layout_constraintLeft_creator="1"
                    tools:layout_constraintRight_creator="1"
                    tools:layout_constraintTop_creator="1" />
            </android.support.constraint.ConstraintLayout>

            <TableRow
                android:id="@+id/puppyRows"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:backgroundTint="@color/cast_expanded_controller_seek_bar_progress_background_tint_color"
                android:orientation="vertical"
                android:visibility="visible" >

                <ListView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:backgroundTint="@color/colorPrimaryDark" >


                </ListView>
            </TableRow>

        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</android.support.design.widget.CoordinatorLayout>
